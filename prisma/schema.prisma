
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model User {
  id String @id
  firstName String @default("Unknow")
  lastName String @default("")
  email String
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())

  @@map("users")
}